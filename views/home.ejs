<%- include("partials/header"); %>
<h1>Home</h1>
<p><%= homeContent %></p>
<% allBlogs.forEach(function(blog){ %>
    <h1>
        <%= blog.title %>
    </h1>
    <p>
        <%= blog.blog.substring(0,100) + " ..." %>
        <a href="/posts/<%= blog.title %>">Read more</a> 
    </p>
<% }); %>
<button class="btn btn-dark newBlog" onclick="location.href='/compose';">Add a new blog</button>
<%- include("partials/footer"); %>